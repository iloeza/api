            <div class="form-group">
                <label for="rol">Rol del usuario</label>
                <input type="rol" class="form-control" id="rol" name="rol" placeholder="Ingresa el rol">
            </div>
            <button id="log" type="submit" class="btn btn-primary">Submit</button>
        </form>

    </div>





